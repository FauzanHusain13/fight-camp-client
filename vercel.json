{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/users/(.*)",
        "dest": "/service/user.js"
      },
      {
        "src": "/api/v1/auth/(.*)",
        "dest": "/service/auth.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }